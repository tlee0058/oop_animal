Assignclass Animal(object):
    def __init__(self, name, health):
        self.name = name
        self.health = health
        
        
    def walk(self):
        self.health -= 1
        return self
        
    def run(self):
        self.health += 5
        return self
        
    def display_health(self):
        print self.health
        return self
    
animal1 = Animal("Jan", 3)
animal1.walk().walk().walk().run().run().display_health()


class Dog(Animal):
    def pet(self):
        self.health = 150
        
    def pet(self):
        self.health += 5
        return self
        
d = Dog("Jimmy", 10)
d.walk().walk().walk().run().run().pet().display_health()
    

